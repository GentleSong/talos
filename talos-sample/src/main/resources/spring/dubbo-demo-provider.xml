<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	<dubbo:application name="talos-sample-provider"/>
	<dubbo:registry address="redis://dubbo-redis:6379"/>
	<dubbo:protocol name="dubbo" port="20880"/>
	<dubbo:service interface="com.kxd.talos.trace.sample.dubbo.DubboService" ref="dubboService"/>
	<bean id="dubboService" class="com.kxd.talos.trace.sample.dubbo.DubboServiceImpl"/>
</beans>